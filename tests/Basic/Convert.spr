//!! -t "SparrowImplicitLib.spr" -fno-main

fun printByte(x: Byte)
    write(" B: ")
    writeLn(x)

fun printUByte(x: UByte)
    write("UB: ")
    writeLn(x)

fun printInt(x: Int)
    write(" I: ")
    writeLn(x)

fun printUInt(x: UInt)
    write("UI: ")
    writeLn(x)

