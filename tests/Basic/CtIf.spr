[native("writeLnFloat64"), autoCt] fun writeLnFloatCt(x: Float)

// Function that can be called only with compile-time arguments
[ct] fun ctTest(x: Float)
    writeLnFloatCt(x)

[ct] if ( true )
    fun my_f { writeLn("f.1"); }
else
    fun my_f { writeLn("f.2"); }

[ct] if ( false )
    fun my_g { writeLn("g.1"); }
else
    fun my_g { writeLn("g.2"); }

[native("test")] fun test(n: Int)
    if ( true )
        ctTest(1.0)
    else
        ctTest(2.0)

    [ct] if ( true )
        ctTest(11.0)
    else
        ctTest(12.0)
    [ct] if ( false )
        ctTest(21.0)
    else
        ctTest(22.0)

    [ct] if ( true )
        writeLn("ok")
    else
        writeLn("fail")
        some error here

    my_f()
    my_g()

