class A
{
    fun ctor
    {
        writeLn("A.ctor");
    }
    fun ctor(n: Int)
    {
        write("A.ctor("); write(n); writeLn(")");
    }
    fun ctor(a: A)
    {
        writeLn("A.ctor(copy)");
    }
    fun =(other: A)
    {
        writeLn("A.=");
    }

    fun dtor
    {
        writeLn("A.dtor");
    }
}

class B
{
    fun ctor
    {
        writeLn("B.ctor");
    }
    fun ctor(n: Int)
    {
        write("B.ctor("); write(n); writeLn(")");
    }
    fun ctor(b: B)
    {
        writeLn("B.ctor(copy)");
    }
    fun =(other: B)
    {
        writeLn("B.=");
    }

    fun dtor
    {
        writeLn("B.dtor");
    }
}

class MyType
{
    fun ctor
    {
        writeLn("MyType.ctor");
    }
    fun ctor(i: Int)
    {
        a.ctor(i);
        writeLn("MyType.ctor(Int)");
    }
    fun ctor(i,j: Int)
    {
        a.ctor(i);
        b.ctor(j);
        writeLn("MyType.ctor(Int, Int)");
    }
    fun ctor(x,y: Double)
    {
        ctor(Int(x), Int(y));
        writeLn("MyType.ctor(Double, Double)");
    }
    fun dtor
    {
        writeLn("MyType.dtor");
    }

    var a: A;
    var b: B;
}

class Empty
{
}

class AlmostEmpty
{
    var a: A;
    var b: B;
}


[native("test")] fun test(n: Int)
{
    if ( n == 1 )
        test1();
    else if ( n == 4 )
        test4();
    else if ( n == 5 )
        test5();
}

fun t1_1()
{
    var o: MyType;
}

fun t1_2()
{
    var o = MyType(10);
}

fun t1_3()
{
    var o = MyType(10, 11);
}

fun t1_4()
{
    var o = MyType(20.0, 21.0);
}


fun test1
{
    t1_1();
    writeLn("---");
    t1_2();
    writeLn("---");
    t1_3();
    writeLn("---");
    t1_4();
}

