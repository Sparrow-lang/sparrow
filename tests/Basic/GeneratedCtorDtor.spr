//!! -t "SparrowImplicitLib.spr" -fno-main

datatype A

fun ctor(this: !A)
    writeLn("A.ctor")

fun ctor(this: !A, n: Int)
    write("A.ctor("); write(n); writeLn(")")

fun ctor(this: !A, a: A)
    writeLn("A.ctor(copy)")

fun dtor(this: !A)
    writeLn("A.dtor")

fun =(this, other: !A)
    writeLn("A.=")

datatype B

fun ctor(this: !B)
    writeLn("B.ctor")

fun ctor(this: !B, n: Int)
    write("B.ctor("); write(n); writeLn(")")

fun ctor(this: !B, b: B)
    writeLn("B.ctor(copy)")

fun dtor(this: !B)
    writeLn("B.dtor")

fun =(this, other: B)
    writeLn("B.=")

datatype MyType
    a: A
    b: B

fun ctor(this: !MyType)
    writeLn("MyType.ctor")

fun ctor(this, other: !MyType)
    ;

fun ctor(this: !MyType, i: Int)
    a.ctor(i)
    writeLn("MyType.ctor(Int)")

fun ctor(this: !MyType, i,j: Int)
    a.ctor(i)
    b.ctor(j)
    writeLn("MyType.ctor(Int, Int)")

fun ctor(this: !MyType, x,y: Float)
    this.ctor(Int(x), Int(y))
    //ctor(this, Int(x), Int(y))
    writeLn("MyType.ctor(Float, Float)")

fun dtor(this: !MyType)
    writeLn("MyType.dtor")

datatype Empty

datatype AlmostEmpty
    a: A
    b: B

[native("test")]
fun test(n: Int)
    if n == 1
        test1
    else if n == 4
        test4
    else if n == 5
        test5

fun t1_1
    let o: MyType

fun t1_2
    let o = MyType(10)

fun t1_3
    let o = MyType(10, 11)

fun t1_4
    let o = MyType(20.0, 21.0)

fun test1
    t1_1
    writeLn("---")
    t1_2
    writeLn("---")
    t1_3
    writeLn("---")
    t1_4

