fun test4
    var x = Empty()
    x.ctor(Empty())

    let y = AlmostEmpty()
    writeLn(">")
    var z: AlmostEmpty
    writeLn(">")
    z.dtor()
    writeLn("copy:")
    z.ctor(y)
    writeLn(">")

