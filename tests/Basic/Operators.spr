package Test;

fun assert(b: Bool)
{
    if ( !b )
    {
        write('F'); write('A'); write('I'); writeLn('L');
    }
}

fun fassert(b: Bool)
{
    if ( ! !b )
    {
        write('F'); write('A'); write('I'); writeLn('L');
    }
}

fun pow1(x: Double, n: Int): Double
{
    if ( n == 0 )
        return 1.0;
    else if ( n == 1 )
        return x;
    else
        return pow1(x*x, n/2) * pow1(x, n%2);
}

fun pow2(x: Double, n: Int): Double
{
    if ( n == 0 )
        return 1.0;
    else
        return x * pow2(x, n-1);
    return x;
}

fun pow3(x: Double, n: Int): Double
{
    var res: Double;
    res = 1.0;
    while ( n>0 ; n=n-1; )
    {
        res = res * x;
    }
    return res;
}

fun pow4(x: Double, n: Int): Double
{
    var res: Double;
    res = 1.0;
    while ( n>0 ; n-=1; )
    {
        res *= x;
    }
    return res;
}

class MyInt
{
    var x: Int;

    fun ctor() { x = 0; }
    fun ctor(other: MyInt) { x = other.x; }
}

fun + (x,y: MyInt): MyInt
{
    var res: MyInt;
    res.x = x.x + y.x;
    return res;
}

fun * (x,y: MyInt): MyInt
{
    var res: MyInt;
    res.x = x.x * y.x;
    return res;
}

fun < (x,y: MyInt): Bool
{
    return x.x < y.x;
}

fun = (x: MyInt@, y: MyInt)
{
    x.x = y.x;
}


