fun assert(b: Bool)
{
    if ( !b )
    {
        write('F'); write('A'); write('I'); writeLn('L');
    }
}

fun fassert(b: Bool)
{
    if ( ! !b )
    {
        write('F'); write('A'); write('I'); writeLn('L');
    }
}

