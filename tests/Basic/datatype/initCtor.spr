// Test purpose: Test initCtor for various datatypes

[convert]
datatype Tracer
    _val: Int

fun ctor(this: !Tracer)
    _val ctor
    writeLn('Tracer.ctor()')

[convert]
fun ctor(this: !Tracer, val: Int)
    _val ctor val
    write('Tracer.ctor(')
    write(val)
    writeLn(')')

fun ctor(this, other: !Tracer)
    _val ctor other._val
    write('Tracer.copy_ctor(')
    write(_val)
    writeLn(')')

fun dtor(this: !Tracer)
    write('Tracer.dtor ')
    writeLn(_val)

[initCtor]
datatype S1
    _v1: Tracer
    _v2: Tracer

[initCtor]
datatype S2
    _v1: Tracer
    _v2: Tracer = 20

[initCtor]
datatype S3
    _v1: Tracer = 10
    _v2: Tracer = 20

[native("test")]
fun test(n: Int)
    ;
        let o1: S1
    writeLn('---')
        let o1: S2
        writeLn('-')
        let o2: S2 = 100
        writeLn('-')
    writeLn('---')
        let o1: S3
    writeLn('------')
        let o1 = S1(50, 60)
        writeLn('-')
        let o2 = o1
        writeLn('-')

