import std.ranges

fun getRangeSize(range: RandomAccessRange): SizeType
    cout << "RandomAccessRange fun selected" << endl
    return range.size()

fun getRangeSize(range: Range): SizeType
    cout << "Range fun selected" << endl
    var n: SizeType = 0
    while !range.isEmpty(); range.popFront()
        ++n
    return n

