import std.ranges;

fun sprMain
{
    let r = 0..10;
    for val = r
    {
        cout << ' ' << val;
    }
    cout << endl;
    for val = r
    {
        cout << ' ' << val;
    }
    cout << endl;
    for val = r
    {
        cout << ' ' << val;
    }
    cout << endl;

    let r2 = 125..130;
    for val = r2
    {
        cout << ' ' << val;
    }
    cout << endl;
    for val: Int = r2
    {
        cout << ' ' << val;
    }
    cout << endl;
    for val: Byte = r2
    {
        cout << ' ' << Int(val);
    }
    cout << endl;
}


