import std.ranges;

fun <<(os: @OutStream, r: Range): typeOf(os) if typeOf(r) != StringRef
{
    var first = true;
    cout << "(";
    for ( x = r )
    {
        if ( !first )
            cout << ", ";
        first = false;
        cout << x;
    }
    cout << ")";
    return os;
}

fun sprMain
{
    var r1 = (1...5) map (fun n = repeat(n*n, n));
    var r2 = r1 map \rangeSize;

    cout << r1 << endl;
    cout << r2 << endl;
}

