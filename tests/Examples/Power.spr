import math;

fun pow(x: Double, n: Int ct): Double
{
    if ( n == 0 )
        return 1.0;
    else if ( n == 1 )
        return x;
    else
        return pow(x*x, n/2) * pow(x, n%2);
}

fun pow(x: Double, n: Int): Double
{
    if ( n == 0 )
        return 1.0;
    else if ( n == 1 )
        return x;
    else
        return pow(x*x, n/2) * pow(x, n%2);
}

fun pow(x: Double, n: Double): Double
{
    return math.pow(x, n);
}

