import Logic.Prolog;

using Logic.*;

compileProlog("
    orbits(mercury, sun).
    orbits(venus, sun).
    orbits(earth, sun).
    orbits(mars, sun).
    orbits(moon, earth).
    orbits(phobos, mars).
    orbits(deimos, mars).

    planet(P) :- orbits(P, sun).
 
    satellite(S) :- orbits(S, P), planet(P).
");

fun test1
{
    var x: LStr;
    var pred = satellite(x);
    var pred = satellite(x,x);  // ERROR
}
