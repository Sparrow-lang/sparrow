// !! -O2

import std.ranges;

fun[noInline] testAccumulate(n: UInt): ULong
{
    var res: ULong = 0;
    for ( i = 0..n )
    {
        for ( j = 0..n )
        {
            res += i*j;
        }
    }
    return res;
}

fun sprMain
{
    if ( programArgs.size() < 2 )
        return;
    var n = programArgs(1) asInt;

    var res = testAccumulate(n);
    cout << res << endl;
}

