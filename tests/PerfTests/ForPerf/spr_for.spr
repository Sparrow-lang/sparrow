// !! -O2

import std.ranges

[noInline] fun testAccumulate(n: UInt): ULong
    var res: ULong = 0
    for i = 0..n
        for j = 0..n
            res += i*j;
    return res;

fun sprMain
    if ( programArgs.size() < 2 )
        return;
    let n = programArgs(1) asInt

    let res = testAccumulate(n)
    cout << res << endl

