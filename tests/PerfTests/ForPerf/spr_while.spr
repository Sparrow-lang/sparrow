//!! -O2

fun testAccumulate(n: UInt): ULong
{
    var res: ULong = 0;
    var i = 0;
    while ( i<n; ++i )
    {
        var j = 0;
        while ( j<n; ++j )
        {
            res += i*j;
        }
    }
    return res;
}

fun sprMain
{
    if ( programArgs.size() < 2 )
        return;
    var n = programArgs(1) asInt;

    var res = testAccumulate(n);
    cout << res << endl;
}

