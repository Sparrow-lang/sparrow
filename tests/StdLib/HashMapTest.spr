fun makeList(): List(Int*Int)
{
    var li = List(Int*Int)();

    li.pushBack(3 ~ 10);
    li.pushBack(145 ~ 42);
    li.pushBack(834 ~ 32);
    li.pushBack(8 ~ 43);
    li.pushBack(73 ~ 4);
    li.pushBack(73 ~ 3);
    li.pushBack(101 ~ 5);
    li.pushBack(4 ~ 312);
    li.pushBack(43 ~ 128);
    li.pushBack(4 ~ 1);
    li.pushBack(106 ~ 32);
    li.pushBack(3 ~ 3);
    li.pushBack(5 ~ 34);
    li.pushBack(53 ~ 54);
    li.pushBack(62 ~ 3);

    return li;
}

fun printPair(p: Int*Int)
{
    cout << p._1;
    cout << "-";
    cout << p._2;
}

fun printRange(nr: AnyType)
{
    var v: Vector(Int*Int) = nr;

    var r = v.all();
    sort(r, (fun (l,r: AnyType) = l._1 < r._1));
    while ( !r.isEmpty() )
    {
        printPair(r.front());
        r.popFront();
        if ( r.isEmpty() )
            cout << endl;
        else
            cout << ' ';
    }
}

