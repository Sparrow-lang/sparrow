fun test19()
{
    var hs = Map(Int, Int)(makeList().all());
    var keys = hs.keys();
    var values = hs.values();

    while ( !keys.isEmpty() )
    {
        cout << keys.front();
        cout << ' ';
        keys.popFront();
    }
    cout << "." << endl;
    while ( !values.isEmpty() )
    {
        cout << values.front();
        cout << ' ';
        values.popFront();
    }
    cout << "." << endl;
}
