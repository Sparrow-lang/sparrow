class IntWrapper
{
    var x: Int;

    fun[convert] ctor(x: Int) { this.x ctor x; }

    fun hash: SizeType
    {
        cout << "IntWrapper.hash called for value " << x << endl;
        return x;
    }
}

class IntWithNoHash
{
    var x: Int;

    fun[convert] ctor(x: Int) { this.x ctor x; }
}

class MyTypeTraits
{
    fun equal(l, r: IntWithNoHash) = l == r;
    fun less(l, r: IntWithNoHash) = l.x < r.x;
    fun hash(val: IntWithNoHash): SizeType
    {
        cout << "MyTypeTraits.hash called for value " << val.x << endl;
        return val.x;
    }
}


