fun printRange(nr: AnyType)
{
    var v: Vector(Int) = nr;

    var r = v.all();
    sort(r);
    while ( !r.isEmpty() )
    {
        cout << r.front();
        r.popFront();
        if (r.isEmpty() )
            cout << endl;
        else
            cout << ' ';
    }
}

