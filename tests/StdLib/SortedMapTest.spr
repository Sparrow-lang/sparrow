fun test19()
{
    var sm = SortedMap(Int, Int, Less(Int), Equal(Int))(makeList().all());
    var keys = sm.keys();
    var values = sm.values();

    while ( !keys.isEmpty() )
    {
        cout << keys.front();
        cout << ' ';
        keys.popFront();
    }
    cout << "." << endl;
    while ( !values.isEmpty() )
    {
        cout << values.front();
        cout << ' ';
        values.popFront();
    }
    cout << "." << endl;
}
/*<<<Keys and values(19)
3 4 5 8 43 53 62 73 101 106 145 834 .
10 312 34 43 128 54 3 4 5 32 42 32 .
>>>*/

