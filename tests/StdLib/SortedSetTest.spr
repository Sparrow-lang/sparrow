    cout << ss2.capacity() << endl;
    cout << ss2.contains(53) << endl;
    cout << ss2.contains(106) << endl;
    cout << ss2.contains(4) << endl;
    cout << ss2.contains(9999) << endl;
    cout << ss.isEmpty() << endl;
    cout << ss.size() << endl;
    cout << ss.capacity() << endl;
    cout << ss.contains(10) << endl;
}
