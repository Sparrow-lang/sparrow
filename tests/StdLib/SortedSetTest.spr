fun printRange(nr: AnyType)
{
    var v: Vector(Int) = nr;

    var r = v.all();
    while !r.isEmpty()
    {
        cout << r.front();
        r.popFront();
        if (r.isEmpty() )
            cout << endl;
        else
            cout << ' ';
    }
}

class Equal(type: Type)
{
    fun ctor() { }
    fun ()(x: type, y: type): Bool { return x == y; }
}

fun test1()
{
